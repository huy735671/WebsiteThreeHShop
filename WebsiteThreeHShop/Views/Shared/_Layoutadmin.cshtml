
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">

    <title>Admin|THREEHSHOP</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta content="Responsive bootstrap 4 admin template" name="description">
    <meta content="Coderthemes" name="author">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/images/favicon.ico">
    <!-- App css -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bootstrap-stylesheet">
    <link href="~/Content/css/icons.min.css" rel="stylesheet" type="text/css">
    <link href="~/Content/css/app.min.css" rel="stylesheet" type="text/css" id="app-stylesheet">

    <script src="~/tinymce_6.2.0/tinymce/js/tinymce/tinymce.min.js"></script>
    <script>tinymce.init({ selection: 'texarea' });</script>
</head>

<body>

    <!-- Begin page -->
    <div id="wrapper">


        <!-- Topbar Start -->
        <div class="navbar-custom">
            <ul class="list-unstyled topnav-menu float-right mb-0">

                

                <li class="dropdown notification-list">
                   
                    <div class="dropdown-menu dropdown-menu-right dropdown-lg">

                        <!-- item-->
                        <div class="dropdown-item noti-title">
                            <h5 class="font-16 m-0">
